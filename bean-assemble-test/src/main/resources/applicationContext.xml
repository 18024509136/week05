<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context.xsd">
    <context:component-scan base-package="com.geek.spring.beanassembletest"/>

    <bean id="demoService1" class="com.geek.spring.beanassembletest.service.impl.DemoServiceImpl"></bean>

    <bean class="com.geek.spring.beanassembletest.service.DemoController" init-method="init" destroy-method="cleanUp">
        <property name="demoService1" ref="demoService1"/>
    </bean>


</beans>